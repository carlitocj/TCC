

%% Contracted forms



%% One-token contracted forms (single word short forms)

d',
de;
de_PREP.

n',
em;
em_PREP.

pl',
por;
por_PREP.

pra,
para;
para_PREP.



%% Token-ambiguous contracted forms

%%consigo,
%%com si;
%%consigo_V//com_PREP si_PRS/.

%%desse,
%%de esse;
%%desse_V//de_PREP esse_POSS#ms/.

%%desses,
%%de esses;
%%desses_V//de_PREP esses_POSS#mp/.

%%deste,
%%de este;
%%deste_V//de_PREP este_POSS#ms/.

%%destes,
%%de estes;
%%destes_V//de_PREP estes_POSS#mp/.

%%mas,
%%me as;
%%mas_CJ//me_CL as_CL/.

%%no,
%%em o;
%%no_CL//em_PREP o_DA#ms/.

%%nos,
%%em os;
%%nos_CL//em_PREP os_DA#mp/.

%%na,
%%em a;
%%na_CL//em_PREP a_DA#fs/.

%%nas,
%%em as;
%%nas_CL//em_PREP as_DA#fp/.

%%pela,
%%por a;
%%pela_V//por_PREP a_DA#fs/.

%%pelas,
%%por as;
%%pelas_V//por_PREP as_DA#fp/.

%%pelo,
%%por o;
%%pelo_V//por_PREP o_DA#ms/.




